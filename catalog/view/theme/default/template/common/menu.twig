							<li>
								<a href="{{ menuLinks.contact.link }}">{{ text_contact}}</a>
							</li>
						{% endif %}
					</ul>
				</div>

				{% if language %}
					{{ language }}
				{% endif %}

				{% if open %}
					<div class="worktime">
						{{ open }}
					</div>
				{% endif %}

				<div class="hcall">
					<a href="tel:{{ telephone }}">
						<svg width="20px" height="24px"><use xlink:href="#hcall"></use></svg>
						<span>{{ telephone }}</span>
					</a>
				</div>
			</div>
		</nav>

	{% endif %}
</div>

<script >
	function headermenu() {
if (jQuery(window).width() < 992) {
jQuery('ul.nav li.dropdown a.header-menu').attr("data-toggle", "dropdown");
} else {
jQuery('ul.nav li.dropdown a.header-menu').attr("data-toggle", "");
}
}
$(document).ready(function () {
headermenu();
});
jQuery(window).resize(function () {
headermenu();
});
jQuery(window).scroll(function () {
headermenu();
});
</script>






